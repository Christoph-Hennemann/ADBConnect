    <!-- DEVELOPER_OPTIONS_DISABLED-->
    <string name="developer_options_disabled_application_state_information">usb debugging disabled</string>
    <string name="developer_options_disabled_execution_source_information">run on device</string>
    <string name="developer_options_disabled_execution_commands_information">
        1. Go to your phone\'s settings\n
        2. Enter the about phone section\n
        3. Scroll down to Build number\n
        4. Click Build number multiple times\n
        5. Go back and enter developer options\n
        6. Enable USB Debugging\n
        7. Come Back…
    </string>
    <string name="developer_options_disabled_execution_commands_information_simple">USB Debugging is disabled</string>

